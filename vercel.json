{
  "version": 2,
  "builds": [
    {
      "src": "next.config.js",
      "use": "@vercel/next",
      "config": {
        "skipBuildRegexp": ".*",
        "maxLambdaSize": "50mb",
        "buildCommand": "CI=false npm run build"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/"
    }
  ],
  "buildSettings": {
    "ignoreErrors": true,
    "ignoreWarnings": true
  },
  "github": {
    "silent": true,
    "autoAlias": true
  }
}
